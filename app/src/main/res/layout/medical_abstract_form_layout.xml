<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp" android:layout_marginBottom="8dp"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:text="Date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/date_textview_medical_abstract_form_layout"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:textSize="18sp" android:textColor="@android:color/black" android:textStyle="bold"
                    android:textAlignment="center"/>
            <TextView
                    android:text="Medical History"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/medical_history_title_textview_medical_abstract_form_layout"
                    app:layout_constraintTop_toBottomOf="@+id/date_textview_medical_abstract_form_layout"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="8dp" android:textSize="18sp" android:textColor="@android:color/black"
                    android:textStyle="bold" android:textAlignment="center"/>
            <TextView
                    android:text="Medical History..."
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/medical_history_textview_textview_medical_abstract_form_layout"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/medical_history_title_textview_medical_abstract_form_layout"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp" android:textSize="16sp"
                    android:textColor="@android:color/black" android:textAlignment="viewStart"/>
            <TextView
                    android:text="Physical Examination"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/physical_examination_title_textview_medical_abstract_form_layout"
                    app:layout_constraintTop_toBottomOf="@+id/medical_history_textview_textview_medical_abstract_form_layout"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp" android:textSize="18sp"
                    android:textStyle="bold" android:textColor="@android:color/black" android:textAlignment="center"/>
            <TextView
                    android:text="Physical Examination..."
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/physical_examination_textview_medical_abstract_form_layout"
                    app:layout_constraintTop_toBottomOf="@+id/physical_examination_title_textview_medical_abstract_form_layout"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="8dp" android:layout_marginStart="16dp" android:layout_marginEnd="16dp"
                    android:textSize="16sp" android:textAlignment="viewStart" android:textColor="@android:color/black"/>
            <TextView
                    android:text="Diagnoses"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/diagnoses_title_textview_textview_medical_abstract_form_layout"
                    app:layout_constraintTop_toBottomOf="@+id/physical_examination_textview_medical_abstract_form_layout"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="8dp" android:textSize="18sp" android:textStyle="bold"
                    android:textColor="@android:color/black" android:textAlignment="center"/>
            <TextView
                    android:text="Diagnoses..."
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/diagnoses_textview_medical_abstract_form_layout"
                    app:layout_constraintTop_toBottomOf="@+id/diagnoses_title_textview_textview_medical_abstract_form_layout"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
                    android:layout_marginTop="8dp" android:textSize="16sp" android:textColor="@android:color/black"
                    android:textAlignment="viewStart"/>
            <TextView
                    android:text="Plans"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/plans_title_textview_textview_medical_abstract_form_layout"
                    app:layout_constraintTop_toBottomOf="@+id/diagnoses_textview_medical_abstract_form_layout"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="8dp" android:textSize="18sp" android:textColor="@android:color/black"
                    android:textStyle="bold" android:textAlignment="center"/>
            <TextView
                    android:text="Plans..."
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/plans_textview_medical_abstract_form_layout" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/plans_title_textview_textview_medical_abstract_form_layout"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp"
                    android:layout_marginStart="16dp" android:textSize="16sp" android:textColor="@android:color/black"
                    android:textAlignment="viewStart" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>