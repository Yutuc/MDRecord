<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".patientfile.ExpandedMedicalAbstractFormActivity">

    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/scrollview_view_medical_abstract_form_pdf">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:id="@+id/constraint_layout_scrollview_view_medical_abstract_form_pdf">

            <TextView
                    android:text="Patient Name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/patient_name_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp" android:textSize="20sp"
                    android:textAlignment="center" android:textStyle="bold" android:textColor="@android:color/black"/>
            <TextView
                    android:text="Age"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/patient_age_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintTop_toBottomOf="@+id/patient_name_textview_view_medical_abstract_form_pdf"
                    android:textColor="@android:color/black"
                    android:textAlignment="viewEnd"
                    app:layout_constraintEnd_toStartOf="@+id/slash_textview_view_medical_abstract_form_pdf"/>
            <TextView
                    android:text="Gender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/patient_gender_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintTop_toBottomOf="@+id/patient_name_textview_view_medical_abstract_form_pdf"
                    android:textAlignment="viewStart" android:textColor="@android:color/black"
                    app:layout_constraintStart_toEndOf="@+id/slash_textview_view_medical_abstract_form_pdf"
            />
            <TextView
                    android:text="/"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/slash_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintTop_toBottomOf="@+id/patient_name_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="@+id/patient_name_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintEnd_toEndOf="@+id/patient_name_textview_view_medical_abstract_form_pdf"
                    android:textColor="@android:color/black" android:textAlignment="center"/>
            <TextView
                    android:text="Address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/patient_address_textview_view_medical_abstract_form_pdf"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/slash_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:textColor="@android:color/black" android:textAlignment="center"/>
            <TextView
                    android:text="Medical History"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/medical_history_title_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintTop_toBottomOf="@+id/patient_address_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp" android:textSize="18sp"
                    android:textStyle="bold" android:textAlignment="center" android:textColor="@android:color/black"/>
            <TextView
                    android:text="Medical History..."
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:id="@+id/medical_history_textview_view_medical_abstract_form_pdf"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/medical_history_title_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:textAlignment="viewStart"
                    android:textColor="@android:color/black" android:textSize="16sp"/>
            <TextView
                    android:text="Physical Examination"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/physical_examination_title_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintTop_toBottomOf="@+id/medical_history_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:textSize="18sp"
                    android:textColor="@android:color/black" android:textStyle="bold" android:textAlignment="center"
                    android:layout_marginTop="8dp"/>
            <TextView
                    android:text="Physical Examination..."
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:id="@+id/physical_examination_textview_view_medical_abstract_form_pdf"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/physical_examination_title_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:textColor="@android:color/black"
                    android:textSize="16sp" android:textAlignment="viewStart"/>
            <TextView
                    android:text="Diagnoses"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/diganoses_title_textview_view_medical_abstract_form_pdf"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/physical_examination_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:textColor="@android:color/black"
                    android:textSize="18sp" android:textAlignment="center" android:textStyle="bold"/>
            <TextView
                    android:text="Diagnoses..."
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:id="@+id/diagnoses_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintTop_toBottomOf="@+id/diganoses_title_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="8dp" android:textSize="16sp" android:textColor="@android:color/black"
                    android:textAlignment="viewStart"/>
            <TextView
                    android:text="Plans"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/plans_title_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintTop_toBottomOf="@+id/diagnoses_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="8dp" android:textSize="18sp" android:textColor="@android:color/black"
                    android:textAlignment="center" android:textStyle="bold"/>
            <TextView
                    android:text="Plans..."
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="16dp"
                    android:paddingEnd="16dp"
                    android:id="@+id/plans_textview_view_medical_abstract_form_pdf" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/plans_title_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" android:textSize="16sp"
                    android:textAlignment="viewStart" android:textColor="@android:color/black"/>
            <TextView
                    android:text="Name of doctor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/name_of_doctor_textview_view_medical_abstract_form_pdf"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/plans_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
                    android:textColor="@android:color/black" android:textAlignment="viewStart"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/license_number_textview_view_medical_abstract_form_pdf"
                    android:textStyle="bold"/>
            <TextView
                    android:text="License number:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/license_number_textview_view_medical_abstract_form_pdf"
                    app:layout_constraintStart_toStartOf="@+id/name_of_doctor_textview_view_medical_abstract_form_pdf"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent" android:textColor="@android:color/black"
                    android:textStyle="bold"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>